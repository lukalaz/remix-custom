datasource db {
  provider = "sqlite"
  url      = "file:/data/sqlite.db"
  // figure out why env file doesn't work
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  slug     String @id
  title    String
  markdown String
  excerpt  String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
